<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.utfpr.guilhermej.sd1.av2.controller.ServerUiController">
   <children>
      <TableView fx:id="stockOrdersTable">
        <columns>
          <TableColumn fx:id="orderPlacerTableColumn" editable="false" minWidth="-1.0" prefWidth="-1.0" text="Placer" />
          <TableColumn fx:id="orderTableColumn" minWidth="-1.0" prefWidth="-1.0" text="Order" />
            <TableColumn prefWidth="75.0" text="Stocks">
               <columns>
                  <TableColumn fx:id="enterpriseTableColumn" minWidth="-1.0" prefWidth="-1.0" text="Enterprise" />
                  <TableColumn fx:id="priceTableColumn" minWidth="-1.0" prefWidth="-1.0" text="Price" />
                  <TableColumn fx:id="quantityTableColumn" minWidth="-1.0" prefWidth="-1.0" text="Quantity" />
               </columns>
            </TableColumn>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</StackPane>
